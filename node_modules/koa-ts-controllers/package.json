    "koa-body": "^4.1.0",
    "koa-bodyparser": "^4.2.1",
    "koa-cookie": "^1.0.0",
    "koa-router": "^7.4.0",
    "lodash": "^4.17.14",
    "reflect-metadata": "^0.1.13"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@types/boom": "^7.2.1",
    "@types/jest": "^24.0.15",
    "@types/koa": "^2.0.49",
    "@types/lodash": "^4.14.136",
    "@types/node": "^12.6.2",
    "jest": "^24.8.0",
    "koa": "^2.7.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  }
